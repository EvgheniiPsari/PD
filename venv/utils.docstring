import datetime

def get_current_date():
    """
    Возвращает текущую дату в формате 'ГГГГ-ММ-ДД'.
    :return: Текущая дата.
    """
    return datetime.date.today()

def square(x):
    """
    Возвращает квадрат числа.
    :param x: Число, для которого нужно вычислить квадрат.
    :return: Квадрат числа x.
    """
    return x ** 2
